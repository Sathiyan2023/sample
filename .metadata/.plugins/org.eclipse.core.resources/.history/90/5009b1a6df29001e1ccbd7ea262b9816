package org.workout;

public class Levi {

	public static void main(String[] args){
		int a[] = new int[5];
		a[0] = 300;
		a[1] = 900;
		a[2] = 200;
		a[3] = 1000;
		a[4] = 700;

		int temp;
			
		for(int i = 0; i < a.length; i++){
			for(int j = i+1; j < a.length; j++){
				if(a[i] < a[j]){
					temp = a[i];	
					a[i] = a[j];
					a[j] = temp;
				}
			}
		System.out.println(a[i]);
		}
	}
}

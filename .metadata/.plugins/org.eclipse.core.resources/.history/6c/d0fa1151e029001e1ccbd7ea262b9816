package org.workout;

public class Levi {

	public static void main(String[] args){
		int a[] = new int[5];
			
		a[0] = 10;
		a[1] = 10;
		a[2] = 10;
		a[3] = 10;
		a[4] = 10;

		int temp;
		
		for(int i = 0; i < a.length; i++){
			for(int j = 1+i; j < a.length; i++){
				if(a[i] < a[j]){
					temp = a[i];
					a[i] = a[j];
					a[j] = temp;
				}
			}	
		}
		System.out.println(a[(a.length)-2]);
	}
}


package org.array;

import java.util.Arrays;

public class Sorting {
	
	public static void main(String[] args){
		
		int a[] = new int[5];
		
		a[0] = 70;
		a[1] = 90;
		a[2] = 30;
		a[3] = 60;
		a[4] = 120;
		
		Arrays.sort(a);
		
		int length = a.length;
		System.out.println("The length of the string is: "+length+"\n");
		
		for(int x : a){
			System.out.println(x);
		}
		
	}
}

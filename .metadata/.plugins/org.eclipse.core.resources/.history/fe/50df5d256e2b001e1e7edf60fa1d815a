package org.test;

public class Student extends School {

	public static void main(String[] args){
		int evenSum = 0, oddSum = 0;
		
		for(int i = 1; i <= 10; i++){
			if(i % 2 == 0){
				evenSum = evenSum + i;
			}
			else{
				oddSum = oddSum + i;
			}
		}
		System.out.println(evenSum);
		System.out.println(oddSum);	
	}
}






package org.array;

import java.util.Arrays;

public class Sorting {
	
	public static void main(String[] args){
		
		int a[] = new int[5];
		
		a[0] = 10;
		a[1] = 20;
		a[2] = 30;
		a[3] = 40;
		a[4] = 50;
		
		Arrays.sort(a);
		
		int length = a.length;
		System.out.println(length);
		
		for(int x : a){
			System.out.println(x);
		}
		
	}
}

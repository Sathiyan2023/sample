package org.test;

public class Student extends School {

	public static void main(String[] args){
		int a = 153, i = 0, j = 0;
		int n = a;
		while(n > 0){
			i = n % 10;
			j = (i*i*i)+j;
			n = n/10;
		}
		if(a == j){
			System.out.println("Amstrong Number: "+j);
		}
		else{
			System.out.println("Not Amstrong Number: "+j);
		}
	}
}






package org.test;

import java.util.ArrayList;
import java.util.List;

public class Student {
	
	public static void main(String[] args){
		
		List l = new ArrayList();
		
		l.add(10);			//a[0]
		l.add(10);			//a[1]
		l.add(30);
		l.add(50);
		l.add(60);
		l.add(90);
		
		System.out.println(l);
		
	}
}





package org.array;

public class ArraySub_Split {
	
	public static void main(String[] args){
		
		String s = "Yume O Akiramete Shinde Kure";
		
		String s1 = s.substring(0, 7);
		System.out.println(s1);
		
		String[] s2 = s.split(" ");
		for(int i = 0; i < s2.length; i++){
			System.out.println(s2[i]);
		}
		
		
		
		
		
		
		
		
		
		
	}
}

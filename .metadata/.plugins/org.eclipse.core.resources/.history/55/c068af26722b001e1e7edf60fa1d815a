package org.test;

public class Student {

	public static void main(String[] args){
		int a[] = {20, 40, 100, 50, 150};
		int temp;
		for(int i = 0; i < a.length; i++){
			for(int j = 1+i; j < a.length; j++){
				if(a[i] < a[j]){
				temp = a[i];
				a[i] = a[j];
				a[j] = temp;
				}
			}
		System.out.println(a[i]);
		}
	}
}






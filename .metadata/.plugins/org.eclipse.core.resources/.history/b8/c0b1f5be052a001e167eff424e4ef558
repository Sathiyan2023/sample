package exercises;

import java.util.ArrayList;
import java.util.List;

public class Colors {
	
	public static void main(String[] args){
		
		List<String> l = new ArrayList<>();
		
		l.add("Red");
		l.add("Black");
		l.add("Orange");
		l.add("Green");
		l.add("White");
		
		for (String x : l) {
			System.out.println(x);
		}
		
		
	}
}

package org.test;

public class Student {

	public static void main(String[] args{
		String s = "malayalam";
		String r = "";
		for(int i = s.length()-1; i >= 0; i++){
			char c = s.charAt(i);
			r = r + c;
		}
		if(s.equals(r)){
			System.out.println("Palindrome String: "+r);
		}
		else{
			System.out.println("Not Palindrome String: "+r);
		}
	}
}
